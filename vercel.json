{
  "crons": [
    {
      "path": "/api/admin?action=scheduler_tick",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/jobs/db-maintenance",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "api/ingest.js": {
      "maxDuration": 60
    },
    "api/admin.js": {
      "maxDuration": 300
    },
    "api/jobs/db-maintenance.js": {
      "maxDuration": 300
    }
  }
}


