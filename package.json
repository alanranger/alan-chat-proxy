{
  "name": "alan-chat-proxy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "echo \"no build step\"",
    "start": "vercel dev",
    "dev": "node local-dev-server.js",
    "call:fn": "node call-fn.cjs",
    "live-sweep": "node scripts/live-sweep.js",
    "validate:event": "ajv validate --spec=draft2020 -c ajv-formats --allow-union-types -s schemas/event.schema.json -r schemas/jsonld-shared.defs.json -d \"generated/*.event.json\"",
    "validate:itemlist": "ajv validate --spec=draft2020 -c ajv-formats --allow-union-types -s schemas/itemlist.schema.json -r schemas/jsonld-shared.defs.json -d \"generated/*.itemlist.json\"",
    "validate:all": "npm run validate:event && npm run validate:itemlist",
    "test:regression": "node regression-test-suite.js",
    "test:baseline": "node capture-baseline.js",
    "test:compare": "node compare-against-baseline.js",
    "test:safety": "node deployment-safety-check.js",
    "test:all": "npm run test:safety && npm run test:regression"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "csv-parser": "^3.2.0",
    "html-to-text": "^9.0.5",
    "jsdom": "^27.0.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-import": "^2.32.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.25.0"
  },
  "overrides": {
    "@csstools/css-color-parser": "3.0.4"
  }
}
